{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "data": {"url": "https://raw.githubusercontent.com/Chokss54/Gun-Violence-in-US-Visualization/main/data/gun-violence-data_clean.csv"},
  "vconcat": [
    {"width": 600,
      "height": 300,
      "encoding": {"x": {"field": "date", "type": "temporal", "scale": {"domain": {"param": "brush"}}}},
      "layer":[
      {
        "mark": "line",
        "encoding": {
          "y": {"field": "count", "type": "quantitative"},
          "color": {"field": "tragedy type", "type": "nominal"},
          "tooltip":[{"field": "count", "type": "quantitative", "title": "count"}]}
      }
      ]
    },
    {
      "width": 600,
      "height": 300,
      "params": [
        {
          "name": "brush",
          "select": {"type": "interval", "encodings": ["x"]}
        }
      ],
      "encoding": {"x": {"field": "date", "type": "temporal"}},
      "layer":[
      {
        "mark": "line",
        "encoding": {
          "y": {"field": "count", "type": "quantitative"},
          "color": {"field": "tragedy type", "type": "nominal"},
          "tooltip":[{"field": "count", "type": "quantitative", "title": "count"}]}
      }
      ]
    }
  ]
}